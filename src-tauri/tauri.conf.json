{
   "build": {
      "beforeDevCommand": "bun run dev",
      "beforeBuildCommand": "bun run build",
      "devPath": "http://localhost:1420",
      "distDir": "../dist",
      "withGlobalTauri": false
   },
   "package": {
      "productName": "inisha",
      "version": "0.0.0"
   },
   "tauri": {
      "allowlist": {
         "all": false,
         "shell": {
            "all": false,
            "open": true
         }
      },
      "bundle": {
         "active": true,
         "targets": "all",
         "identifier": "com.tauri.dev",
         "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
         ]
      },
      "security": {
         "csp": null
      },
      "windows": [
         {
            "fullscreen": false,
            "maximizable": false,
            "resizable": true,
            "title": "inisha",
            "width": 1600,
            "height": 975,
            "center": true,
            "minWidth": 1024,
            "minHeight": 975,
            "maxWidth": 1600,
            "maxHeight": 975
         }
      ]
   }
}
